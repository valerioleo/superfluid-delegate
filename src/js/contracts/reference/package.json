{
  "name": "@superfluid-delegates/contracts",
  "author": "Valerio Leo @valerioHQ",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "rm -rf deployments/hardhat && rm -rf deployments/localhost && hardhat node",
    "start:test": "npx hardhat node --tags Seed",
    "migrate:hardhat": "npx hardhat --network hardhat deploy",
    "migrate:mumbai": "npx hardhat deploy --network polygonMumbai --tags PrepareTestnet",
    "migrate:live": "npx hardhat --network mainnet deploy",
    "compile": "hardhat compile --show-stack-traces",
    "test": "rm -rf deployments/hardhat && hardhat test",
    "coverage": "scripts/coverage.sh",
    "postinstall": "yarn compile"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "4.5.0",
    "@openzeppelin/contracts-upgradeable": "^4.5.2",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "5.0.0",
    "@types/chai": "^4.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.4",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.6.0",
    "hardhat": "^2.10.2",
    "hardhat-deploy": "^0.9.14",
    "hardhat-gas-reporter": "^1.0.6",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
